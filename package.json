{
  "name": "bun-js-executor",
  "version": "1.0.0",
  "description": "Backend application that executes JavaScript code using Bun",
  "main": "src/server.ts",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun run --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target bun",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["bun", "javascript", "executor", "api", "typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vm2": "^3.9.19",
    "xmlhttprequest": "^1.8.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "@types/xmlhttprequest": "^1.8.2",
    "@types/js-yaml": "^4.0.5",
    "typescript": "^5.0.0"
  }
}